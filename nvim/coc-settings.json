{
    // coc
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "*"
    ],
    "outline.sortBy": "position",
    "outline.splitCommand": "botright 50vs",
    "semanticTokens.filetypes": [
        "go",
        "lua",
        "rust",
        "swift",
        "ts",
    ],
    "suggest.noselect": true,
    "suggest.virtualText": true,
    // coc-eslint
    "eslint.autoFixOnSave": true,
    // coc-explorer
    "explorer.openAction.strategy": "previousBuffer",
    // coc-go
    "go.goplsOptions": {
        "gofumpt": true,
        "usePlaceholders": true,
        "completeUnimported": true
    },
    // coc-rust-analizer
    "rust-analyzer.checkOnSave.command": "clippy",
    // coc-snippets
    "snippets.ultisnips.directories": [
        "~/.local/share/nvim/site/pack/packer/start/vim-snippets/UltiSnips"
    ],
    // coc-solargraph
    "solargraph.diagnostics": true,
    "solargraph.formatting": true,
    // coc-sourcekit
    "sourcekit.sdk": "iphonesimulator",
    "sourcekit.targetArch": "x86_64-apple-ios14.5-simulator",
}
